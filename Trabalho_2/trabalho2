.text
	jal ReadNumbers
	move $t0,$v0
	jal ReadNumbers
	move $t1,$v0
	jal ReadNumbers
	move $t2,$v0
	

#	jal CheckPrimeBruteForce
	
	jal Mode
	j EXIT
	
	
	ReadNumbers:
	li $v0,5
	syscall
	jr $ra
	
	Mode:
	div $t0,$t2
	mfhi $t0
	jr $ra
	
	CheckPrimeBruteForce:
	li $t3,2
	div $t2,$t3
	mfhi $t4
	jr $ra
	
	EXIT: #program exit
	
#	(https://stackoverflow.com/questions/5281779/c-how-to-test-easily-if-it-is-prime-number)